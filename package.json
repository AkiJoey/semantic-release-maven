{
  "name": "semantic-release-maven",
  "version": "1.0.0",
  "description": "semantic-release plugin to publish a maven package.",
  "keywords": [
    "semantic-release"
  ],
  "repository": "github:akijoey/semantic-release-maven",
  "homepage": "https://github.com/akijoey/semantic-release-maven#readme",
